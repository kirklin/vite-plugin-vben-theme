{
  "name": "@kirklin/vite-plugin-vben-theme",
  "version": "0.1.0",
  "packageManager": "pnpm@7.11.0",
  "description": "Vite plugin for dynamically changing the theme color of the interface",
  "author": "Kirk Lin <linkirk@163.com> (https://github.com/kirklin)",
  "license": "MIT",
  "keywords": [
    "vite"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "es",
    "CHANGELOG.md",
    "README.md",
    "README.zh_CN.md"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf es",
    "dev": " tsup src/index.ts --dts --format cjs,esm --watch",
    "dev:client": "tsc -w --p tsconfig.client.json",
    "build": "npm run clean  && tsup src/index.ts --dts --format cjs,esm && tsc -p tsconfig.client.json",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "example:dev": "npm -C example run serve",
    "example:build": "npm -C example run build",
    "release:prepare": "shipjs prepare",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "vite": ">=3.2.0"
  },
  "dependencies": {
    "@types/node": "^14.17.1",
    "@types/tinycolor2": "^1.4.2",
    "clean-css": "^5.1.2",
    "debug": "^4.3.2",
    "esbuild": "^0.11.23",
    "esbuild-plugin-alias": "^0.1.2",
    "picocolors": "^1.0.0",
    "tinycolor2": "^1.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@kirklin/eslint-config": "^0.2.1",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/less": "^3.0.2",
    "@types/node": "^14.17.1",
    "@vue/compiler-sfc": "^3.0.11",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^8.28.0",
    "fs-extra": "^9.1.0",
    "less": "^4.1.1",
    "lint-staged": "^10.5.4",
    "pnpm": "^7.17.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.4.0",
    "typescript": "^4.2.4",
    "vite": "^3.2.0"
  }
}
